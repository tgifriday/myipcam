{
  "name": "UberCam",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "My IP Cam",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "sandbox": {
        "pages": ["IPCam.html"]
  },
  "permissions": [
    {"fileSystem": ["write"]},
    "unlimitedStorage",
     "webview",
     "<all_urls>"
  ]
}
